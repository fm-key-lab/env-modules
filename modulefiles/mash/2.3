#%Module
##
## mash (v2.3)

proc ModulesHelp { } {
   puts stderr "
Adds mash to your path
Version 2.3
"
}

module-whatis "Adds mash to your path\n"
module-whatis "Provides: mash"

set basedir [getenv "MODULE_BASEDIR"]
if { $basedir eq "" } {
    puts stderr "Error: MODULE_BASEDIR environment variable is not set."
    exit 1
}
prepend-path PATH "${basedir}/mash/2.3/bin"