#%Module
##
## musket (v1.1)

proc ModulesHelp { } {
   puts stderr "
Adds musket to your path
Version: 1.1
"
}

module-whatis "Adds musket to your path\n"
module-whatis "Provides: musket <-- should edit to list executables"

set basedir [getenv "MODULE_BASEDIR"]

# TODO: Should be `$basedir`
if { $basedir eq "" } {
    puts stderr "Error: MODULE_BASEDIR environment variable is not set."
    exit 1
}
prepend-path PATH "${basedir}/musket/1.1/musket-1.1/"